{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.ts"],
  "out": "docs",
  "name": "TS Mixins",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeInternal": true,
  "readme": "README.md",
  "plugin": ["typedoc-plugin-markdown"],
  "theme": "default",
  "categorizeByGroup": true,
  "categoryOrder": [
    "Core",
    "Decorators",
    "Configuration",
    "Tracking",
    "Types",
    "Interfaces",
    "Utils",
    "*"
  ]
}
